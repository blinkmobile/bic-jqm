{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "semistandard"
  ],
  "env": {
    "amd": true
  },
  "globals": {
    /* Blink */
    "BMP": true,
    "BlinkForms": true
  },
  "parserOptions": {
    "ecmaVersion": 5
  },
  "plugins": [
    "node"
  ],
  "rules": {
    "node/no-missing-require": 0,
    "node/no-unpublished-require": 0
  }
}
